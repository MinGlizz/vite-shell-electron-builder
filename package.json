{
  "name": "vite-shell-electron-builder",
  "version": "0.0.1",
  "description": "A vite plugin shell electron builder",
  "main": "dist/index.js",
  "scripts": {
    "dev": "yarn build --watch --ignore-watch examples",
    "build": "tsup src/index.ts --dts --format cjs,esm",
